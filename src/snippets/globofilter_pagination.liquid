{% if paginate.pages > 1 %}
  <ul class="pagination">
    {% unless paginate.previous.is_link %}
    <li class="disabled"><span>&laquo;</span></li>
    {% else %}
    <li class="disabled"><a onclick="setPage({{paginate.current_page | minus: 1}})" href="javascript:;" rel="next">&laquo;</a></li>
    {% endunless %}
    {% for part in paginate.parts %}
    {% if part.is_link %}
    	<li><a onclick="setPage({{part.title}})" href="javascript:;">{{part.title}}</a></li>
    {% elsif part.title == '&hellip;' %}
    	<li><span>{{part.title}}</span></li>
    {% else %}
    	<li class="active"><span>{{part.title}}</span></li>
    {% endif %}
	{% endfor %}
    {% unless paginate.next.is_link %}
    <li class="disabled"><span>&raquo;</span></li>
    {% else %}
    <li><a onclick="setPage({{paginate.current_page | plus: 1}})" href="javascript:;" rel="next">Â»</a></li>
    {% endunless %}
  </ul>
{% endif %}
