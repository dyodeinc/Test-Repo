{% if settings.enable_giveaway_page %}
<main id="contact" class="merged-page-template">
  {% include 'breadcrumb' %}

  {%comment%}{% include 'page-header' %}{%endcomment%}

  
  {%if  settings.enable_gym_banner_giveaway_banner_section %}
  <section id="page-header">
  <div class="a-set-feel-banner-img" style="position: relative;"> 
      <img src="{{ settings.gym_banner_giveaway_banner_image | img_url: 'master' }}" alt="feel_banner_img" class="a-set-img" />      
            <h1>{{ page.title }}</h1>
<!--             {{ settings.gym_banner_giveaway_banner_image_title }}  -->
  </div>
  </section>
  {% endif %}
  
  {% if settings.giveaways_countdown %}
    {% include 'countdown' %} 
  {% endif %} 
  
  <section id="get-in-touch" class="section">
    
    <div class="page-width">
      
      <div class="grid-set giveway-set">
<!--         <div class="col-md-12 set_form"> -->
          
          
   <div class="grid__item-setf set_form">       
        
    
        <form id="email_signup" class="klaviyo_standard_embed_LmH6LU contact_form" action="//manage.kmail-lists.com/subscriptions/subscribe" data-ajax-submit="//manage.kmail-lists.com/ajax/subscriptions/subscribe" method="GET" target="_blank" novalidate="novalidate">
         
            <div class="grid__item-setf">
              <div class="set_input name_fld txt-centr">
              <input required type="text" id="ContactFormName" name="contact[name]" placeholder="Enter Name" value="">
           </div>
              </div>
            <input type="hidden" name="g" value="LmH6LU">
              <div class="grid__item-setf">
                <div class="set_input email_fld txt-centr">
                  
                    
    
    <input type="email" value="" name="email" id="k_id_email" placeholder="Your email" />
  
                  
<!--               <input required type="email" id="ContactFormEmail" name="contact[email]" placeholder="Enter Email Address" autocorrect="off" autocapitalize="off" value="{% if form.email %}{{ form.email }}{% elsif customer %}{{ customer.email }}{% endif %}" class="{% if form.errors contains 'email' %}input--error{% endif %}" > -->
           	</div>
         </div>
	  <div class="klaviyo_messages">
    <div class="success_message" style="display:none;"></div>
    <div class="error_message" style="display:none;"></div>
    </div>
          
         <div class="grid__item-setf">
           
           <div class="set_input name_fld txt-centr">

            
    <button type="submit" class="btn contactbtn" style="color: #fff;background-color: #000;">ENTER TO WIN</button>
             {{ settings.gym_banner_giveaway_btn_underneath_text }}
  </div>
<!-- 			              <div class="set_input name_fld txt-centr">

          <input type="submit" class="btn contactbtn" value="Enter To Win">
					</div> -->
           </div>
           
        
<!--       </div> -->
     </form>
    </div>
    
       <div class="grid__item medium-up--one-half contactonright ">
         {% if page.content.size > 0 %}
        <div class="rte">
          {{ page.content }}
        </div>
      {% endif %}
      </div>
  </div>
</div>
  </section>

    
</main>

