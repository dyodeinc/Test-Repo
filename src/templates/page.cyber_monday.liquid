  <div class="section page hot_deals_page">
    <div class="page-width">


        <div class="collection-items-holder hot-deals ">
          <ul class="collection-items grid grid--uniform">
             <div class="row ">
             {% assign product_count = collections[settings.cybermonday-col].products_count %}
             {% paginate collections[settings.cybermonday-col].products by product_count %}
            {%  for product in collections[settings.cybermonday-col].products  %}
            <li class="grid__item small--one-half medium-up--one-quarter">
              {% include 'a-product-card-grid-2' %}
            </li>
            {% endfor %}
            {% endpaginate %}
            </div>
          </ul>
 {% comment %}
           <div class="AjaxinatePagination text-center">
                 <div class="blog-view-all">
                  {% if paginate.next %}
                    <a class="ld ld-ring ld-spin" href="{{ paginate.next.url }}"></a>
                  {% endif %}
