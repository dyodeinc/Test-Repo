{% if settings.enable_cyberpage %}
<main class="{{page.title | handle }} merged-page-template">
  {% include 'breadcrumb' %}
 {% if settings.enable_cybertophead %}

  <section id="page-header">
    <div class="header-background">
      <div class="changable-image" style="background-image: url({{ settings.monday_banner | img_url: 'master' }});">
        <div class="page-width">
          <div class="rowxzx">
               {%if  settings.enable_monbanhead %}

              <h1>{{ page.title }}</h1>
                {% endif %}
          </div>
        </div>
      </div>
    </div>
  </section>
  {% endif %}
  {% unless page.content == blank %}
  <div class="section page ">
    <div class="page-width">
      <div class="rowxc">
          <div class="content text-center">
            {{ page.content }}
          </div>
      </div>
    </div>
  </div>
  {% endunless %}
   {% if settings.enable_top_ban %}
  {% unless settings.enable_cybertophead %}
  <section class="top_bar_monday"  id="page-header">
      <div class=" top_image_ban full_width">

        <img src="{{ settings.top_monday_banner | img_url: 'master' }}" alt="About WODLife" class="about-image" />
