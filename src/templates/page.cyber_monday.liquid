    <div class="page-width">

      <div class="rowxcxc">
       <div class="grid grid--uniform">

        <div class="grid__item extra-small--one-whole small--one-half medium-up--one-quarter">
          <i class="fa {{ settings.cicon1 }}"></i>
          <h4>{{ settings.ctitle1 }}</h4>
          <div class="description">
            {{ settings.csubtitle1 }}
          </div>
        </div>
        <div class="grid__item extra-small--one-whole small--one-half medium-up--one-quarter">
          <i class="fa {{ settings.cicon2 }}"></i>
          <h4>{{ settings.ctitle2 }}</h4>
          <div class="description">
            {{ settings.csubtitle2 }}
          </div>
        </div>
        <div class="grid__item extra-small--one-whole small--one-half medium-up--one-quarter">
          <i class="fa {{ settings.cicon3 }}"></i>
          <h4>{{ settings.ctitle3 }}</h4>
          <div class="description">
            {{ settings.csubtitle3 }}
          </div>
        </div>
        <div class="grid__item extra-small--one-whole small--one-half medium-up--one-quarter">
          <i class="fa {{ settings.cicon4 }}"></i>
          <h4>{{ settings.ctitle4 }}</h4>
          <div class="description">
            {{ settings.csubtitle4 }}
          </div>
        </div>
        </div>
      </div>
    </div>
  </section>
  {% endif %}

  {% if settings.cyber_monday_countdown %}
