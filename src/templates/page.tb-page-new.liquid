                <a class="tbtwl-apparel-href-img" href="{{settings.tbn_image3_1_link}}">
                  <img src="{{ settings.tbn_image3_1 | img_url: 'original' }}" alt="About WODLife" class="app-coll-img" />
                </a> 
                <div class="tbtwl-apparel-2div_overlay">
                  <p class="carton-title">
                MEN'S
                </p>
                <p>
                <a href="{{settings.tbn_image3_1_link}}"  class="button twl-btn">
                    EXPLORE
                  </a>
                </p>
                  </div>
                  
                </div>

              </div>
              
              
              <div class="grid__item medium-up--one-half small--one-whole extra-small--one-whole twl-img-col">
               <div class="tbtwl-apparel-2div">
                <a class="tbtwl-apparel-href-img" href="{{settings.tbn_image3_2_link}}">
                  <img src="{{ settings.tbn_image3_2 | img_url: 'original' }}" alt="About WODLife" class="app-coll-img" />
                </a> 
                <div class="tbtwl-apparel-2div_overlay">
                <p class="carton-title">
                  WOMEN'S
                  </p>
                <p>
                  <a href="{{settings.tbn_image3_2_link}}"  class="button twl-btn">
                    EXPLORE
                  </a>
                    </p>
                 </div>
              </div>
              </div>
              
            </div>
            {% endif %}
          </div>
        </div>
    </div>
  
  
  <div class="grid grid--uniform page hot_deals_page">
    {% if settings.enable_new_tb_apparel_linklist %}
     <div class="grid__item medium-up--one-fifth tab-down--one-whole filter-menu-wrapper pg-shop-by-cat">
      <div class="filter-menu-holder">
              <div class="apparel-cat">
            <h2 class="collection-title apparel-category-title">       
              {{ settings.new_tb_apparel_linklist_title }}
            </h2>
              </div>
            <div class="widget-content">

              <ul class="collection-filter-list collection-page-list">
                {% for link in linklists.[settings.new_tb_apparel_linklist].links %}
                <li>
                    <a href="{{ link.url }}">{{ link.title }}</a>
                </li>
                {% endfor %}
              </ul>


            </div>    
                 </div>       
              </div>
    {% endif %}
    
    <div class="grid__item {% if settings.enable_new_tb_apparel_linklist %} medium-up--four-fifths{% else %} medium-up--one-whole {% endif %} tab-down--one-whole collection-list app-coll">
<!--     <div class="container">   -->
     <div class="collection-grid">
       {% paginate collections[settings.tbmainmonday-col].products %}
        <div class="">
       <div class="collection-items-holder hot-deals auto-load-col">
          <div class="division-wrap">
                <h1 class="division-title">
                    {{ settings.tbmaincollection_man }}
                </h1>
              </div>
          <div class="grid grid--uniform twl_clear_class collection-items">
             {%  for product in collections[settings.tbmainmonday-col].products  %}
            <div class=" grid__item extra-small--one-whole tab-down--one-third small--one-half medium-up--one-quarter">
              {% include 'a-product-card-grid-2' %}
            </div>
            {% endfor %}            
          </div>
        
         {% comment %}
          <div class="grid grid--uniform twl_clear_class collection-items">
            {%  for product in collections['the-wod-life-mens-t-shirts'].products  %}
            <div class=" grid__item extra-small--one-whole tab-down--one-third small--one-half medium-up--one-quarter">
              {% include 'a-product-card-grid-2' %}
            </div>
            {% endfor %}
          </div>
         {% endcomment %}
              
                <div class="stance-view-all-button">
                  <a href="{{ settings.tbmainmonday_col_men_link }}">{{ settings.tbmainmonday_col_view_all_men }}</a>
                </div>
<!--         </div> -->
         </div>
          </div>
             

      {% endpaginate %}
      </div>
    
      
      
<!--     </div> -->
    
<!--     <div class="container">   -->
      
      
      
       <div class="collection-grid">
         {% paginate collections[settings.tbappmonday-col-women].products %}
 <div class="">
        <div class="collection-items-holder hot-deals auto-load-col">
          <div class="division-wrap">
                <h1 class="division-title">
                      {{ settings.tbmaincollection_woman }}
                </h1>
              </div>
<!--            <h1>{{ settings.tbmaincollection_woman }}</h1> -->
          <div class="grid grid--uniform twl_clear_class collection-items">
            
            {%  for product in collections[settings.tbmainmonday-col-women].products  %}
             <div class=" grid__item extra-small--one-whole tab-down--one-third small--one-half medium-up--one-quarter">
              {% include 'a-product-card-grid-2' %}
            </div>
            {% endfor %}
           
         </div> 
          <div class="stance-view-all-button">
                  <a href="{{ settings.tbmainmonday_col_women_link }}">{{ settings.tbmainmonday_col_view_all_women }}</a>
                </div>
        </div>
         </div>
                 

      {% endpaginate %}
      </div>
      
      
    </div>
<!--     </div> -->
  </div>
  </div>
  
</main>
 {% endif %}

