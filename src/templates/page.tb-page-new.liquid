{{ 'flipclock.css' | asset_url | stylesheet_tag }}
{% if settings.enable_tbmainpage %}
<main class="{{page.title | handle }} merged-page-template">
  {% include 'breadcrumb' %}
  
 {% if settings.enable_tbmaintophead %}

 {%comment%} <section id="page-header">
    
<div class="header-background easter-bg tbpagebanner" {%if  settings.new_tb_banner %} style="background-image: url({{ settings.new_tb_banner | img_url: 'master' }});" {% endif %}>
      <div class="page-width">
      <div class="changable-image">
        <div class="container">
          <div class="grid">
            <div class="grid__item">
                {%if  settings.enable_tb_bandhead %}

                  <h1>{{ page.title }}</h1>
                {% endif %}
                {%if  settings.enable_tb_bandhead_customhead %}

                  <h1>{{ settings.enable_tb_bandhead_customhead_text }}</h1>
                {% endif %}
                 {%if  settings.enable_tb_bandhead_customhead_subheadtext %}
                   <h3 class="subtext-fortbpage">{{ settings.enable_tb_bandhead_customhead_subheadtext }}</h3>
                 {% endif %}

            </div>
          </div>
        </div>
      </div>
    </div>
    </div>

  </section>{%endcomment%}
  
  <div class="bann_image">
    <img src="{{ settings.new_tb_banner | img_url: 'master' }}" alt="About WODLife" class="about-image" />
   
  <div class="bann_img-cont">
                {%if  settings.enable_tb_bandhead %}

                  <h2 style="color:#000">{{ page.title }}</h2>
                {% endif %}
                {%if  settings.enable_tb_bandhead_customhead %}

                  <h2 style="color:#000">{{ settings.enable_tb_bandhead_customhead_text }}</h2>
                {% endif %}
                 {%if  settings.enable_tb_bandhead_customhead_subheadtext %}
                   <h3 style="color:#000" class="subtext-fortbpage">{{ settings.enable_tb_bandhead_customhead_subheadtext }}</h3>
                 {% endif %}
  </div>
  </div>
  
  
  
  {% endif %}
  
  {% if settings.new_tb_apparel_countdown %}
    {% include 'countdown' %} 
  {% endif %} 

  {% unless page.content == blank %}
  <div class="section page ">
    <div class="page-width">
      <div class="grid">
        <div class="grid__item">
          <div class="content text-center">
            {{ page.content }}
          </div>
        </div>
      </div>
    </div>
  </div>
  {% endunless %}
  
  
  
 
