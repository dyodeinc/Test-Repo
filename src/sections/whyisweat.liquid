<div class="main-block">
         
  
 {% for block in section.blocks %}
  
   {% if block.settings.video_enable %}
  
     {% if block.settings.video_title != "" %}
          <div class="section-header text-center whyis_heading">
        		<h1>{{ block.settings.video_title }}</h1>
          </div>
    {% endif %}
          
      {% if block.settings.video_url != "" %}
     	<div class="product-sigle-video-sec">
          <div class="video-wrapper whyisweatvideo">
              <iframe class="feature-row__image" src="{{ block.settings.video_url }}" style="width: 100%; height: 100%" frameborder="0" allowfullscreen=""></iframe>
          </div>
        </div>
     {% endif %} 
  
   {% endif %} 
  
  {% endfor %}
     
</div>


{% schema %}
  {
    "name": "Video List",
    "class": "video-section-area",
    "blocks": [
      {
        "type": "video-list",
        "name": "Video List",
        "settings": [
          {
            "type": "checkbox",
            "id": "video_enable",
            "label": "Enable This page",
            "default": true
      },
          {
            "type": "text",
            "id": "video_title",
            "label": "Video Title"
          },
{
            "type": "text",
            "id": "video_url",
            "label": "Video Url"
          }
        ]
      }
    ]
  }
{% endschema %}

