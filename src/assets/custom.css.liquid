.main-collection-template .collection-ctn .filter-menu .filter-group .collection-filter-title:after,
.mob-filter-by-panle .panel--sec .filter-menu .collection-filter .collection-filter-title:after {
  position:absolute; width:10px;
  height:2px; content:''; background:#000; display:block;
  right:0;
  top: 50%;
  -ms-transform: translateY(-50%);
  -webkit-transform: translateY(-50%);
  -moz-transform: translateY(-50%);
  -o-transform: translateY(-50%);
  transform: translateY(-50%);
}
.main-collection-template .collection-ctn .filter-menu .filter-group .collection-filter-title:before,
.mob-filter-by-panle .panel--sec .filter-menu .collection-filter .collection-filter-title:before {
  position:absolute; width:2px;
  height:10px; content:''; background:#000; display:block;
  right:4px;
  top: 50%;
  -ms-transform: translateY(-50%);
  -webkit-transform: translateY(-50%);
  -moz-transform: translateY(-50%);
  -o-transform: translateY(-50%);
  transform: translateY(-50%);
}
.mob-filter-by-panle .panel--sec .filter-menu .collection-filter .collection-filter-title:after {
  width: 7px;
  height: 1px;
}
.mob-filter-by-panle .panel--sec .filter-menu .collection-filter .collection-filter-title:before {
  width: 1px;
  height: 8px;
  right: 3px;
}
.mob-filter-by-panle .panel--sec .filter-menu .collection-filter.has_group_selected .collection-filter-title:before {
  display: none;
}
.mob-filter-by-panle .panel--sec .filter-menu .collection-filter.pt-group-expanded .collection-filter-title:before {
